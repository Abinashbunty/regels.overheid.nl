@prefix dcat: <http://www.w3.org/ns/dcat#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

#
# In DCAT you can specify a dcat:distribution using a Blank Node (BNode) to represent the distribution details within a dataset.
# This approach is particularly useful when you want to describe distributions that are specific to a particular dataset 
# but do not have their own unique identifiers (IRIs).
#
# Catalog definition
<https://regels.overheid.nl/register> a dcat:Catalog;
    dct:title "Open Regels van de Nederlandse Overheid";
    dct:description "Open Regels catalogiseert welke regels er zijn, van wie de regels zijn en met welk doel ze worden gebruikt";
    dct:publisher <https://www.rijksoverheid.nl/ministeries/ministerie-van-binnenlandse-zaken-en-koninkrijksrelaties>;
    dct:issued "2023-01-01"^^xsd:date;
    dct:modified "2024-07-08"^^xsd:date;
    dcat:dataset <https://regels.overheid.nl/register/indInkToesl>.

#
# Dataset definition
<https://regels.overheid.nl/register/indInkToesl> a dcat:Dataset;
    dct:title "Individuele inkomenstoeslag - regelgroepen in ALEF";
    dct:description "Verzameling regels voor 'Individuele inkomenstoeslag' van de Gemeente Utrecht die horen bij het object: 'Aanvraag'";
    dct:issued "2023-06-12"^^xsd:date;
    dct:modified "2024-01-05"^^xsd:date;
    dct:identifier "indInkToesl";
    dct:license <https://creativecommons.org/licenses/by-nc-sa/4.0/>;
    dct:creator [
        a foaf:Person;
        foaf:name "Steven M. Gort";
        foaf:homepage <https://regels.overheid.nl/person/steven-gort>;
    ];
    dct:subject [
        a skos:Concept;
        skos:prefLabel "Individuele Inkomsenstoeslag";
        skos:exactMatch <https://regels.overheid.nl/subject/individuele-inkomenstoeslag>;
    ];
    dct:publisher [
        a foaf:Organization;
        foaf:name "Gemeente Utrecht";
        foaf:homepage <https://www.utrecht.nl>;
    ];
    dcat:theme [
        a skos:Concept;
        skos:prefLabel "Sociale Zekerheid";
        skos:exactMatch <https://regels.overheid.nl/theme/sociale-zekerheid>;
    ];
    dcat:distribution [
        a dcat:Distribution;
        dct:title "HTML weergave van de ALEF regelgroep";
        dcat:accessURL <https://regels.overheid.nl/register/indInkToesl-regelgroep>;
        dcat:mediaType "text/html";
        dct:format "HTML";
        dct:issued "2024-01-05"^^xsd:date;
    ], [
        a dcat:Distribution;
        dct:title "HTML weergave van de parameters van de ALEF regelgroep";
        dcat:accessURL <https://regels.overheid.nl/register/indInkToesl-parameters>;
        dcat:mediaType "text/html";
        dct:format "HTML";
        dct:issued "2024-05-01"^^xsd:date;
    ], [
        a dcat:Distribution;
        dct:title "Gegevensmodel voor het object: 'Aanvraag'";
        dcat:accessURL <https://regels.overheid.nl/register/indInkToesl-gegevensmodel>;
        dcat:mediaType "text/html";
        dct:format "HTML";
        dct:issued "2024-05-01"^^xsd:date;
    ], [
        dct:title "Service 'Berekenen_IIT'";
        dcat:endpointURL <https://viloketdemo.blueriq.com/runtime/webservices/aaas/BerekenInkomensToeslag/Berekenen_IIT/aaas.wsdl>;
        dcat:servesDataset <https://regels.overheid.nl/register/indInkToesl>;
        dcat:mediaType "application/soap+xml";
        dct:format "SOAP";
        dct:issued "2024-05-01"^^xsd:date;
        dct:publisher [
            a foaf:Organization;
            foaf:name "Blueriq";
            foaf:homepage <https://www.blueriq.com/en/>;
        ];
    ].
