    skos:prefLabel "Wetsanalyse (JAS)"@nl, "Legal Analysis (JAS)"@en.

ex:WetsanalyseJRM a skos:Concept;
    skos:inScheme ronl:AnalysisConcept;
    skos:prefLabel "Wetsanalyse (JRM)"@nl, "Legal Analysis (JRM)"@en.

ex:FLINT a skos:Concept;
