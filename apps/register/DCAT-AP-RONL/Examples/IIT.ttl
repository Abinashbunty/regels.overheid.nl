    dcat:mediaType "text/csv" ;
    dct:format "CSV" ;
    dct:issued "2024-07-01"^^xsd:date .

# Distribution 2: Paraameters
<https://data.example.org/distribution/health-stats-json> a dcat:Distribution ;
    dct:title "Health Statistics JSON Distribution" ;
    dcat:accessURL <https://data.example.org/dataset/health-stats.json> ;
    dcat:mediaType "application/json" ;
    dct:format "JSON" ;
    dct:issued "2024-07-01"^^xsd:date .

# Distribution 3: Gegevensmodel
<https://data.example.org/distribution/health-stats-xml> a dcat:Distribution ;
    dct:title "Health Statistics XML Distribution" ;
    dcat:accessURL <https://data.example.org/dataset/health-stats.xml> ;
    dcat:mediaType "application/xml" ;
    dct:format "XML" ;
    dct:issued "2024-07-01"^^xsd:date .

# Distribution 4: Regel Service (API)
<https://data.another.org/service/health-stats-api> a dcat:DataService ;
    dct:title "Health Statistics API" ;
    dcat:endpointURL <https://api.another.org/health-stats> ;
    dcat:servesDataset <https://data.example.org/dataset/health-statistics> ;
    dcat:mediaType "application/json" ;
    dct:format "API" ;
    dct:issued "2024-07-01"^^xsd:date ;
    dct:publisher <https://data.another.org/organization/another-organization> .

