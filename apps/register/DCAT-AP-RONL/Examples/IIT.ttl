# Catalog definition
<https://regels.overheid.nl/register> a dcat:Catalog;
    dct:title "Open Regels van de Nederlandse Overheid";
    dct:description "Open Regels catalogiseert welke regels er zijn, van wie de regels zijn en met welk doel ze worden gebruikt";
    # The entity responsible for publishing the catalog
    dct:publisher <https://www.rijksoverheid.nl/ministeries/ministerie-van-binnenlandse-zaken-en-koninkrijksrelaties>;
    dct:issued "2023-01-01"^^xsd:date;
    dct:modified "2024-07-08"^^xsd:date;
    dcat:dataset <https://regels.overheid.nl/register/indInkToesl>.

# Dataset definition
<https://regels.overheid.nl/register/indInkToesl> a dcat:Dataset;
    dct:title "Individuele inkomenstoeslag - regelgroepen in ALEF";
    dct:description "Verzameling regels voor 'Individuele inkomenstoeslag' van de Gemeente Utrecht die horen bij het object: 'Aanvraag'";
    dct:issued "2023-06-12"^^xsd:date;
    dct:modified "2024-01-05"^^xsd:date;
    dct:identifier "indInkToesl";
    dcat:distribution <https://regels.overheid.nl/register/indInkToesl-regelgroep>,
        <https://regels.overheid.nl/register/indInkToesl-parameters>,
        <https://regels.overheid.nl/register/indInkToesl-gegevensmodel>,
        <https://regels.overheid.nl/register/indInkToesl-api>;
    dct:publisher <https://www.utrecht.nl/>;
    dct:creator <https://regels.overheid.nl/person/steven-gort>;
    # dct:subject is used to provide detailed subject information about the dataset's content.
    dct:subject <https://regels.overheid.nl/subject/individuele-inkomenstoeslag>;
    dct:license <https://regels.overheid.nl/license/CC-BY-NC-SA>;
    dcat:keyword "Inidividuele inkomenstoeslag",
        "Toeslagen",
        "IIT",
        "Werk en Inkomen";
    # dcat:theme is used to facilitate dataset discovery by grouping datasets into thematic categories within a catalog.
    dcat:theme <https://regels.overheid.nl/theme/sociale-zekerheid>.

# Distribution 1: Regelgroep
<https://regels.overheid.nl/register/indInkToesl-regelgroep> a dcat:Distribution;
    dct:title "HTML weergave van de ALEF regelgroep";
    dcat:accessURL <https://regels.overheid.nl/register/indInkToesl>;
    dcat:mediaType "text/html";
    dct:format "HTML";
    dct:issued "2024-01-05"^^xsd:date.

# Distribution 2: Parameters
<https://regels.overheid.nl/register/indInkToesl-parameters> a dcat:Distribution;
    dct:title "HTML weergave van de parameters van de ALEF regelgroep";
    dcat:accessURL <https://regels.overheid.nl/register/indInkToesl>;
    dcat:mediaType "text/html";
    dct:format "HTML";
    dct:issued "2024-05-01"^^xsd:date.

# Distribution 3: Gegevensmodel
<https://regels.overheid.nl/register/indInkToesl-gegevensmodel> a dcat:Distribution;
    dct:title "Gegevensmodel voor het object: 'Aanvraag'";
    dcat:accessURL <https://regels.overheid.nl/register/indInkToesl>;
    dcat:mediaType "text/html";
    dct:format "HTML";
    dct:issued "2024-05-01"^^xsd:date.

# Distribution 4: Regel Service (API)
<https://regels.overheid.nl/register/indInkToesl-api> a dcat:DataService;
    dct:title "Health Statistics API";
    dcat:endpointURL <https://viloketdemo.blueriq.com/runtime/webservices/aaas/BerekenInkomensToeslag/Berekenen_IIT/aaas.wsdl>;
    dcat:servesDataset <https://regels.overheid.nl/register/indInkToesl>;
    dcat:mediaType "application/soap+xml";
    dct:format "SOAP";
    dct:issued "2024-05-01"^^xsd:date;
    dct:publisher <https://www.blueriq.com/en/>.

# Publisher (organization) definition
<https://www.utrecht.nl/> a foaf:Organization;
    foaf:name "Gemeente Utrecht".

<https://www.blueriq.com/en/> a foaf:Organization;
    foaf:name "Blueriq".

# Creator (person) definition
<https://regels.overheid.nl/person/steven-gort> a foaf:Person;
    foaf:name "Steven M. Gort".

# Subject (concept) definition
<https://regels.overheid.nl/subject/individuele-inkomenstoeslag> a skos:Concept;
    skos:prefLabel "Individuele Inkomsenstoeslag".

# License definition
<https://regels.overheid.nl/license/CC-BY-NC-SA> a dct:LicenseDocument;
    dct:title "Creative Commons license BY-NC-SA 4.0";
    dct:identifier "https://creativecommons.org/licenses/by-nc-sa/4.0/".

# Theme definition
<https://regels.overheid.nl/theme/sociale-zekerheid> a dcat:Theme;
    dct:title "Sociale Zekerheid".

